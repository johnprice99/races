{% extends '::base.html.twig' %}

{% block title %}Races{% endblock %}

{% block body %}
	{% for race in races %}
		<div>
			<p><a href="{{ path('race_view', {'id' : race.id}) }}">{% include('JPRaceBundle:Race:title.html.twig') %}{% if race.complete %} COMPLETE{% endif %}</a></p>
		</div>
	{% endfor %}
{% endblock %}